{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [],
  "opts": {
    "destination": "docs/jsdoc",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}